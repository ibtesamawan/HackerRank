#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    int d1 = n % 10;  // accessing last digit
    n = n / 10;  // removing digiut

    int d2 = n % 10;
    n = n / 10;

    int d3 = n % 10;
    n = n / 10;

    int d4 = n % 10;
    n = n / 10;

    int d5 = n % 10;

    int sum = d1 + d2 + d3 + d4 + d5;

    printf("%d", sum);

    return 0;
}

#include <stdio.h>

int main() {
    char ch;
    char s[20];     // to store a word 
    char sen[100];  // to store a full sentence 

    scanf("%c", &ch);
    scanf("%s", s);
    fgets(sen, 100, stdin);  // 
    fgets(sen, 100, stdin);  // reads the sentence from input

    printf("%c\n", ch);
    printf("%s\n", s);
    printf("%s", sen);

    return 0;
}

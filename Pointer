#include <stdio.h>

void update(int *a, int *b) {
    int x = *a + *b;         // a' = a + b
    int y = *a - *b;         // a - b

    if (y < 0) {
        y = -y;              // absolute difference
    }

    *a = x;
    *b = y;
}

int main() {
    int a, b;
    scanf("%d %d", &a, &b);

    update(&a, &b);

    printf("%d\n", a);
    printf("%d\n", b);

    return 0;
}
